{
  "name": "grounded",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:kill": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && vite",
    "dev:port": "vite --port 3001",
    "dev:tauri": "tauri dev",
    "build": "vite build",
    "build:pwa": "vite build && npm run package",
    "build:desktop": "vite build && tauri build",
    "build:android": "vite build && npx cap sync android && cd android && ./gradlew assembleRelease",
    "build:android:debug": "vite build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "build:all": "npm run build:desktop && npm run build:android",
    "preview": "vite preview",
    "package": "node scripts/package.js",
    "tauri": "tauri"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@xenova/transformers": "^2.17.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-tauri": "^4.0.0"
  }
}
